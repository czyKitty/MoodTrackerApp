<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.moodtracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MoodTracker">
        <activity
            android:name=".ui.login.GoogleSignInActivity"
            android:label="FirebaseLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="package.OtherActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

        </activity>
        <activity
            android:name=".ui.track.LineChartActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".ui.track.PieChartActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".ui.track.WordNegActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".ui.track.WordPosActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".ui.home.HomeFragment"
            android:label="@string/app_name">
        </activity>
    </application>

</manifest>